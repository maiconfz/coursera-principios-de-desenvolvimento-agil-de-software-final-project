@startuml Diagrama de classes

class Leitor {
    - nome: String
    - email: String
    - senha: String
    - pontos: int
    - livrosLidos: List<Livro>
    - trofeusConquistados: List<Trofeu>
}

class Livro {
    - titulo: String
    - descricao: String
    - autor: String
    - editora: String
    - ano: int
    - quantidadePaginas: int
    - estilo: Estilo
}

class Estilo {
    - nome: String
}

class Trofeu {
    - nome: String
}

